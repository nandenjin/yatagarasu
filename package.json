{
  "name": "ytgrs",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src",
    "lintfix": "eslint --ext ts,tsx src --fix",
    "format": "prettier --write '**/*.{ts,tsx,scss,md,json,yml,yaml}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.7.1",
    "consola": "2.15.3",
    "firebase": "9.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6"
  },
  "devDependencies": {
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vitejs/plugin-react": "1.1.3",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1",
    "sass": "1.45.1",
    "typescript": "4.5.4",
    "vite": "2.7.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,scss,md,json,yml,yaml}": "prettier --write"
  }
}
