{
  "name": "ytgrs",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src",
    "lintfix": "eslint --ext ts,tsx src --fix",
    "format": "prettier --write '**/*.{ts,tsx,scss,md,json,yml,yaml}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.3",
    "consola": "2.15.3",
    "firebase": "9.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vitejs/plugin-react": "2.0.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "1.54.0",
    "typescript": "4.7.4",
    "vite": "3.0.2"
  },
  "resolutions": {
    "@types/react": "18.0.15"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,scss,md,json,yml,yaml}": "prettier --write"
  }
}
