{
  "name": "ytgrs",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src",
    "lintfix": "eslint --ext ts,tsx src --fix",
    "format": "prettier --write '**/*.{ts,tsx,scss,md,json,yml,yaml}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "consola": "2.15.3",
    "firebase": "9.6.11",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-redux": "8.0.0"
  },
  "devDependencies": {
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "@vitejs/plugin-react": "1.3.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "sass": "1.50.1",
    "typescript": "4.6.3",
    "vite": "2.9.5"
  },
  "resolutions": {
    "@types/react": "18.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,scss,md,json,yml,yaml}": "prettier --write"
  }
}
