{
  "name": "ytgrs",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext ts,tsx src",
    "lintfix": "eslint --ext ts,tsx src --fix",
    "format": "prettier --write '**/*.{ts,tsx,scss,md,json,yml,yaml}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.2",
    "consola": "2.15.3",
    "firebase": "9.8.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.2"
  },
  "devDependencies": {
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "sass": "1.52.2",
    "typescript": "4.7.3",
    "vite": "2.9.10"
  },
  "resolutions": {
    "@types/react": "18.0.12"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,scss,md,json,yml,yaml}": "prettier --write"
  }
}
